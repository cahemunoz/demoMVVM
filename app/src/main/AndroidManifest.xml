<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.outsmart.baseprojectkotlin">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:name="com.outsmart.baseprojectkotlin.ExampleApplication"
            android:theme="@style/AppTheme">
        <activity android:name="com.outsmart.baseprojectkotlin.presentation.screens.time_ticker.TimeTickerActivity">
        </activity>
        <activity android:name="com.outsmart.baseprojectkotlin.presentation.screens.user_list.UserCrudActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>